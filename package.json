{
  "name": "scriptable-js",
  "version": "0.1.0",
  "description": "Scriptable.run javascript client",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./dist/main.js",
  "types": "dist/types.d.ts",
  "module": "dist/module.js",
  "scripts": {
    "build": "yarn generate && yarn parcel build",
    "generate": "yarn swagger-typescript-api --axios -p openapi.yaml -o ./src -n ./index.ts --templates ./apiClientTemplates --union-enums --module-name-first-tag --route-types --extract-request-params --extract-request-body --extract-response-body --extract-response-error --api-class-name Scriptable"
  },
  "resolutions": {
    "swagger-typescript-api/**/typescript": "4.7.4"
  },
  "peerDependencies": {
    "axios": "*"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "buffer": "^6.0.3",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "swagger-typescript-api": "^10.0.1",
    "typescript": ">=3.0.0"
  }
}
